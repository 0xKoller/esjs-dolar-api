{
  "outputDirectory": "dist/static/",
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    {
      "source": "/v1/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/cron/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/",
      "destination": "/docs/"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/docs/sitemap.xml"
    }
  ]
}
