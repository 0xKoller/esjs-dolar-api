{
  "outputDirectory": "dist/static/",
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/servidor/**"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/docs/"
    },
    {
      "source": "/cron/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/:path*",
      "destination": "/docs/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "cl.dolarapi.com"
        }
      ],
      "destination": "/cl/:path*"
    }
  ]
}
