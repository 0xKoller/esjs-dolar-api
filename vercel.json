{
  "outputDirectory": "dist/static/",
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/servidor/**"
    }
  },
  "rewrites": [
    {
      "source": "/v1/:path*",
      "has": [
        {
          "type": "host",
          "value": "dolarapi.com"
        }
      ],
      "destination": "/ar/v1/:path*"
    },
    {
      "source": "/v1/:path*",
      "has": [
        {
          "type": "host",
          "value": "cl.dolarapi.com"
        }
      ],
      "destination": "/cl/v1/:path*"
    },
    {
      "source": "/v1/:path*",
      "has": [
        {
          "type": "host",
          "value": "ve.dolarapi.com"
        }
      ],
      "destination": "/ve/v1/:path*"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "cl.dolarapi.com"
        }
      ],
      "destination": "https://cl.dolarapi.com/docs/chile/"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "ve.dolarapi.com"
        }
      ],
      "destination": "https://ve.dolarapi.com/docs/venezuela/"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "dolarapi.com"
        }
      ],
      "destination": "https://dolarapi.com/docs/"
    },
    {
      "source": "/openapi.json",
      "has": [
        {
          "type": "host",
          "value": "dolarapi.com"
        }
      ],
      "destination": "https://dolarapi.com/docs/openapi.json"
    },
    {
      "source": "/openapi.json",
      "has": [
        {
          "type": "host",
          "value": "cl.dolarapi.com"
        }
      ],
      "destination": "https://cl.dolarapi.com/docs/chile/openapi.json"
    },
    {
      "source": "/openapi.json",
      "has": [
        {
          "type": "host",
          "value": "ve.dolarapi.com"
        }
      ],
      "destination": "https://ve.dolarapi.com/docs/venezuela/openapi.json"
    },
    {
      "source": "/favicon.ico",
      "destination": "/docs/favicon.ico"
    },
    {
      "source": "/cron/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "headers": [
    {
      "source": "/v1/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        }
      ]
    }
  ]
}
