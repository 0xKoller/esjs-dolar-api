{
  "outputDirectory": "public/",
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    {
      "source": "/v1/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/cron/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/docs/(.*)",
      "destination": "/index.html"
    }
  ]
}
