v1.obtener('/cotizaciones/:monedaOrigen/:monedaDestino', moneda)
v1.obtener('/cotizaciones/:monedaOrigen/:monedaDestino/', moneda)

v1.use('/historicos/*', cache(60))
v1.obtener('/historicos/dolares/:casa', historicosDolaresCasa)
