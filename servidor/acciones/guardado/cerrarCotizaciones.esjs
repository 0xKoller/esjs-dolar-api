importar { extraerCotizaciones } desde '@/acciones/extraccion/extraerCotizaciones.esjs'
importar { guardarCotizaciones } desde '@/db/database.js'
importar { startOfDay, subHours } desde 'date-fns'
importar { zonedTimeToUtc } desde 'date-fns-tz'

exportar asincrono funcion cerrarCotizaciones() {
  const cotizaciones = esperar extraerCotizaciones()

  const hoyUTC = crear Fecha()

  const hoy = zonedTimeToUtc(startOfDay(subHours(hoyUTC, 3)))

  esperar guardarCotizaciones(cotizaciones, hoy)
}
