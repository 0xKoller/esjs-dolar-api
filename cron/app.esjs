importar ar desde '@/ar/ar.cron.esjs'
importar cl desde '@/cl/cl.cron.esjs'
importar ve desde '@/ve/ve.cron.esjs'
importar uy desde '@/uy/uy.cron.esjs'
importar { grupo } desde '@/log.esjs'

exportar asincrono funcion iniciar(comando) {
  const log = grupo({ cron: 'app.esjs' })

  log.info('Iniciando cron')

  esperar ar()
  esperar cl()
  esperar ve()
  esperar uy()

  log.info('Cron finalizado')
}
