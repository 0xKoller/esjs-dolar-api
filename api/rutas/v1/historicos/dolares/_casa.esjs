importar { obtenerHistoricosPorCasa } desde '@/datos/datos.esjs'

exportar porDefecto asincrono (c) => {
  const dolares = esperar obtenerHistoricosPorCasa(c.req.param('casa'))

  retornar c.json(
    dolares.mapear((dolar) => ({
      moneda: dolar.moneda,
      casa: dolar.casa,
      nombre: dolar.nombre,
      compra: dolar.compra,
      venta: dolar.venta,
      fecha: dolar.fecha,
    })),
  )
}
