{
  "name": "esjs-dolar-api",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run docs:build && pnpm run data:copy && pnpm run api:build",
    "serve": "vite preview",
    "api:build": "vite build --config vite.api.config.js --ssr --mode=production",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:i18n-extract": "npx vue-i18n-extract report --vueFiles './docs/.vitepress/**/*.?(js|vue|md)' --languageFiles './docs/.vitepress/theme/locales/*.?(json|yml|yaml)'",
    "gen:docs": "pnpm run gen:operations && pnpm run gen:ambito && pnpm run gen:cl",
    "gen:operations": "node ./scripts/generate-operations.js",
    "gen:ambito": "node ./scripts/generate-ambito.js",
    "gen:cl": "node ./scripts/generate-cl.js",
    "prettier": "prettier --write '**/*.esjs'",
    "data:copy": "pnpm run data:copy:ar && pnpm run data:copy:cl",
    "data:copy:ar": "mkdir -p ./dist/static/ar/v1 && cp -r ./datos/v1/* ./dist/static/ar/v1",
    "data:copy:cl": "mkdir -p ./dist/static/cl/v1 && cp -r ./datos/cl/v1/* ./dist/static/cl/v1",
    "cron:build": "vite build --config vite.config.js --ssr --mode=production",
    "cron:run": "node ./dist/cron/index.js",
    "cron:test": "vitest --config vite.config.js",
    "cron:test:ui": "vitest --ui",
    "cron:test:run": "vitest run",
    "og:dev": "x-satori -t ./og/Template.vue -c ./og/config.ts --dev",
    "og:build": "x-satori -t ./og/Template.vue -c ./og/config.ts -o image.svg",
    "og:svg": "tsx ./og/gen-svg.mts",
    "og:png": "tsx ./og/gen-png.mts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.2.1",
    "@egoist/tailwindcss-icons": "^1.7.4",
    "@es-js/vite-plugin-esjs": "0.1.0-alpha.9",
    "@hono/vite-dev-server": "^0.0.12",
    "@iconify-json/mdi": "^1.1.66",
    "@intlify/unplugin-vue-i18n": "^1.6.0",
    "@resvg/resvg-js": "^2.6.2",
    "@types/node": "^18.19.31",
    "@vueuse/core": "^10.9.0",
    "autoprefixer": "latest",
    "axios": "^1.6.8",
    "cheerio": "1.0.0-rc.12",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "hono": "^3.12.12",
    "iconv-lite": "^0.6.3",
    "postcss": "latest",
    "prettier": "3.0.3",
    "prettier-plugin-esjs": "0.1.0-alpha.9",
    "sitemap": "^7.1.1",
    "tailwindcss": "latest",
    "tsx": "^3.14.0",
    "typescript": "^5.4.5",
    "vercel": "^32.7.2",
    "vite": "^4.5.3",
    "vitepress": "^1.1.4",
    "vitepress-theme-openapi": "0.0.2-alpha.19",
    "vitest": "^0.34.6",
    "vue": "^3.4.26",
    "vue-i18n": "^9.13.1",
    "x-satori": "0.1.5"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
