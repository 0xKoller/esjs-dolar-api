{
  "name": "esjs-dolar-api",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run docs:build && pnpm run data:copy && pnpm run api:build",
    "serve": "vite preview",
    "api:build": "vite build --ssr --mode=production",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:i18n-extract": "npx vue-i18n-extract report --vueFiles './docs/.vitepress/**/*.?(js|vue|md)' --languageFiles './docs/.vitepress/theme/locales/*.?(json|yml|yaml)'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "md": "node ./docs/scripts/generate-md.js",
    "prettier": "prettier --write '**/*.esjs'",
    "data:copy": "cp -r ./datos/v1 ./dist/static"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.27",
    "@egoist/tailwindcss-icons": "^1.7.1",
    "@es-js/vite-plugin-esjs": "^0.0.15",
    "@hono/vite-dev-server": "^0.0.12",
    "@iconify-json/mdi": "^1.1.63",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@vueuse/core": "^10.5.0",
    "autoprefixer": "latest",
    "cors": "^2.8.5",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.25.2",
    "hono": "^3.9.0",
    "iconv-lite": "^0.6.3",
    "postcss": "latest",
    "prettier": "3.0.3",
    "prettier-plugin-esjs": "^0.0.2",
    "sitemap": "^7.1.1",
    "tailwindcss": "latest",
    "typescript": "^5.2.2",
    "vercel": "^32.5.0",
    "vite": "^4.4.9",
    "vitepress": "^1.0.0-rc.31",
    "vitepress-theme-openapi": "0.0.2-alpha.16",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  }
}
