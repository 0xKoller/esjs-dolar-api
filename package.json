{
  "name": "esjs-dolar-api",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run api:build && pnpm run docs:build",
    "serve": "vite preview",
    "generate": "drizzle-kit generate:sqlite",
    "push": "drizzle-kit push:sqlite",
    "studio": "drizzle-kit studio",
    "api:build": "vite build --ssr --mode=production",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:i18n-extract": "npx vue-i18n-extract report --vueFiles './docs/.vitepress/**/*.?(js|vue|md)' --languageFiles './docs/.vitepress/theme/locales/*.?(json|yml|yaml)'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "md": "node ./docs/scripts/generate-md.js",
    "prettier": "prettier --write '**/*.esjs'"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.27",
    "@es-js/vite-plugin-esjs": "^0.0.15",
    "@hono/vite-dev-server": "^0.0.12",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@vueuse/core": "^10.5.0",
    "autoprefixer": "latest",
    "cheerio": "1.0.0-rc.12",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "hono": "^3.9.0",
    "iconv-lite": "^0.6.3",
    "postcss": "latest",
    "prettier": "3.0.3",
    "prettier-plugin-esjs": "^0.0.1",
    "sitemap": "^7.1.1",
    "tailwindcss": "latest",
    "typescript": "^5.2.2",
    "vavite": "^3.0.0",
    "vercel": "^32.5.0",
    "vite": "^4.4.9",
    "vitepress": "^1.0.0-rc.31",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0"
  },
  "dependencies": {
    "@libsql/client": "^0.3.6-pre.1",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6"
  }
}
