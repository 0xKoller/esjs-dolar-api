{
  "name": "esjs-dolar-api",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run docs:build && pnpm run data:copy && pnpm run api:build",
    "serve": "vite preview",
    "api:build": "vite build --config vite.api.config.js --ssr --mode=production",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:i18n-extract": "npx vue-i18n-extract report --vueFiles './docs/.vitepress/**/*.?(js|vue|md)' --languageFiles './docs/.vitepress/theme/locales/*.?(json|yml|yaml)'",
    "docs:gen": "pnpm run gen:ar && pnpm run gen:cl && pnpm run gen:ve && pnpm run gen:uy",
    "gen:ar": "node --experimental-loader esjs-loader scripts/generadores/generar.ar.esjs",
    "gen:cl": "node --experimental-loader esjs-loader scripts/generadores/generar.cl.esjs",
    "gen:ve": "node --experimental-loader esjs-loader scripts/generadores/generar.ve.esjs",
    "gen:uy": "node --experimental-loader esjs-loader scripts/generadores/generar.uy.esjs",
    "prettier": "prettier --write '**/*.esjs'",
    "data:copy": "pnpm run data:copy:ar && pnpm run data:copy:cl && pnpm run data:copy:ve && pnpm run data:copy:uy",
    "data:copy:ar": "mkdir -p ./dist/static/ar/v1 && cp -r ./datos/v1/* ./dist/static/ar/v1",
    "data:copy:cl": "mkdir -p ./dist/static/cl/v1 && cp -r ./datos/cl/v1/* ./dist/static/cl/v1",
    "data:copy:ve": "mkdir -p ./dist/static/ve/v1 && cp -r ./datos/ve/v1/* ./dist/static/ve/v1",
    "data:copy:uy": "mkdir -p ./dist/static/uy/v1 && cp -r ./datos/uy/v1/* ./dist/static/uy/v1",
    "cron:build": "vite build --config vite.config.js --ssr --mode=production",
    "cron:run": "node ./dist/cron/index.js",
    "cron:test": "vitest --config vite.config.js",
    "cron:test:ui": "vitest --ui",
    "cron:test:run": "vitest run",
    "og:dev": "x-satori -t ./og/Template.vue -c ./og/config.ts --dev",
    "og:build": "x-satori -t ./og/Template.vue -c ./og/config.ts -o image.svg",
    "og:svg": "tsx ./og/gen-svg.mts",
    "og:png": "tsx ./og/gen-png.mts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.27",
    "@egoist/tailwindcss-icons": "^1.7.1",
    "@es-js/vite-plugin-esjs": "0.1.0-alpha.13",
    "@hono/vite-dev-server": "^0.0.12",
    "@iconify-json/mdi": "^1.1.63",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@resvg/resvg-js": "^2.4.1",
    "@types/node": "^18.17.1",
    "@vueuse/core": "^10.5.0",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.7",
    "cheerio": "1.0.0-rc.12",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "esjs-loader": "0.1.0-alpha.0",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.25.2",
    "hono": "^3.9.0",
    "iconv-lite": "^0.6.3",
    "postcss": "^8.4.38",
    "prettier": "3.0.3",
    "prettier-plugin-esjs": "0.1.0-alpha.13",
    "sitemap": "^7.1.1",
    "tailwindcss": "^3.4.3",
    "tsx": "3.12.7",
    "typescript": "^5.2.2",
    "vercel": "^32.5.0",
    "vite": "^4.4.9",
    "vitepress": "^1.2.2",
    "vitepress-theme-openapi": "0.0.2-alpha.19",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0",
    "x-satori": "0.1.5"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  }
}
